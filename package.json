{
  "name": "devsession",
  "displayName": "DevSession",
  "description": "Continue your coding session seamlessly across devices. Restore opened files and cursor positions exactly where you left off.",
  "version": "0.0.1",
  "engines": {
    "vscode": ">=1.107.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
  "viewsContainers": {
    "activitybar": [
      {
        "id": "devsession",
        "title": "DevSession",
        "icon":"src/assets/icon.png"
      }
    ]
  },
  "views": {
    "devsession": [
      {
        "id": "devsession.view",
        "name": "Session"
      }
    ]
  },
  "commands": [
    {
      "command": "devsession.helloWorld",
      "title": "Smart"
    }
  ]
}
,
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
